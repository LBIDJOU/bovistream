{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "node dist/server/node-build.mjs",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-fileupload": "^1.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.7.22",
    "@swc/core": "^1.11.24",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/vue-query": "^5.59.20",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "@types/three": "^0.176.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vueuse/core": "^11.1.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "globals": "^15.9.0",
    "lucide-vue-next": "^0.462.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "serverless-http": "^3.2.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.11",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.176.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3",
    "vue": "^3.4.21",
    "vue-router": "^4.4.5",
    "vite": "^6.2.2",
    "vitest": "^3.1.4"
  }
}
